#
# Sync Point Sur's xband videos to ~/Sync
#
# June-2022, Pat Welch, pat@mousebrains.com

[Unit]
Description=Sync Point Sur's xband videos to Sync

[Timer]
# dayOfWeek Year-Month-Day Hour:Minute:Second
# sync data every 1 hour
OnCalendar=*-*-* 01/1:15:00 UTC
Persistent=true

[Install]
WantedBy=timers.target

